<!-- MODAL PICO -->


<div class="modal fade" id="exampleModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog modal-xl modal-dialog-centered">
    <div class="modal-content">
      <div class="modal-header">
        <h1 class="modal-title fs-5 ">Título de tema de investigación: <input id="titulo-picoc" style="width: 600px" required></h1>
        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
      </div>
      <div class="modal-body">
        <table>
          <thead>
            <tr>
              <th>PICOC</th>
              <th>PREGUNTASㅤ<i style="color: rgb(255, 220, 81)" class="bi bi-question-circle-fill"></i></th>
              <th>PALABRAS CLAVEㅤ<i style="color: rgb(255, 220, 81)" class="bi bi-question-circle-fill"></i></th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <td>Población</td>
              <td><textarea class="form-control" id="pregunta-población-picoc" required></textarea></td>
              <td><textarea class="form-control" id="población-picoc" required></textarea></td>
            </tr>
            <tr>
              <td>Intervención</td>
              <td><textarea class="form-control" id="pregunta-intervencion-picoc" required></textarea></td>
              <td><textarea class="form-control" id="intervencion-picoc" required></textarea></td>
            </tr>
            <tr>
              <td>Comparación</td>
              <td><textarea class="form-control" id="pregunta-comparacion-picoc" required></textarea></td>
              <td><textarea class="form-control" id="comparacion-picoc" required></textarea></td>
            </tr>
            <tr>
              <td>Resultado</td>
              <td><textarea class="form-control" id="pregunta-resultado-picoc" required></textarea></td>
              <td><textarea class="form-control" id="resultado-picoc" required></textarea></td>
            </tr>
            <tr>
              <td>Contexto</td>
              <td><textarea class="form-control" id="pregunta-contexto-picoc" required></textarea></td>
              <td><textarea class="form-control" id="contexto-picoc" required></textarea></td>
            </tr>
          </tbody>
        </table>
        <hr>
        <td>Ecuación de búsqueda</td>
        <td><textarea class="form-control" id="ecuacion-picoc" required></textarea></td>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
        <button id="copiarTextoPicoc" type="button" class="btn btn-danger">Copiar ecuación</button>
        <button id="generarEcuacion" type="button" class="btn btn-primary">Generar ecuación</button>
        <script src="/js/ecuacion.js"></script>
      </div>
    </div>
  </div>
</div>


<!-- MODAL DE INICIO DE PAGINA -->

<div class="pop-up">
        <div class="pop-up-wrap">
            <div id="carouselExampleCaptions" class="carousel slide">
                <div class="carousel-indicators">
                  <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="0" class="active" aria-current="true" aria-label="Slide 1"></button>
                  <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="1" aria-label="Slide 2"></button>
                  <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="2" aria-label="Slide 3"></button>
                  <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="3" aria-label="Slide 4"></button>
                </div>
                <div class="carousel-inner">
                  <div class="carousel-item active">
                    <img src="images/project/project-image06.png" class="d-block w-100" alt="1">
                    <div class="carousel-caption d-none d-md-block">
                      <h5>First slide label</h5>
                      <p>Some representative placeholder content for the first slide.</p>
                    </div>
                  </div>
                  <div class="carousel-item">
                    <img src="images/project/project-image06.png" class="d-block w-100" alt="2">
                    <div class="carousel-caption d-none d-md-block">
                      <h5>Second slide label</h5>
                      <p>Some representative placeholder content for the second slide.</p>
                    </div>
                  </div>
                  <div class="carousel-item">
                    <img src="images/project/project-image06.png" class="d-block w-100" alt="3">
                    <div class="carousel-caption d-none d-md-block">
                      <h5>Third slide label</h5>
                      <p>Some representative placeholder content for the third slide.</p>
                    </div>
                  </div>
                  <div class="carousel-item">
                    <img src="images/project/project-image06.png" class="d-block w-100" alt="">
                    <div class="carousel-caption d-none d-md-block">
                      <h5>Third slide label</h5>
                      <p>Some representative placeholder content for the third slide.</p>
                    </div>
                  </div>
                </div>
                <i class="bi bi-x-square-fill" id="close"></i>
                <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide="prev">
                  <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                  <span class="visually-hidden">Previous</span>
                </button>
                <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide="next">
                  <span class="carousel-control-next-icon" aria-hidden="true"></span>
                  <span class="visually-hidden">Next</span>
                </button>
              </div>
            </div>
        </div>
    </div>

